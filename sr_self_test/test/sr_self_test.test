<launch>
    <!-- Run a small bag in a loop that publishes diagnostics of the hand.  -->
    <node pkg="rosbag" type="play" args="--loop $(find sr_self_test)/test/shadowhand.bag" name="rosbag" />

    <node name="automatic_response" pkg="sr_self_test" type="automatic_manual_test_response.py" />

    <test test-name="test_self_test" pkg="sr_self_test" type="test_self_test" time-limit="2400.0"/>

</launch>
